{
  "name": "izmir",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec babel-node src/index.js",
    "dev:test": "npm run test && chokidar '**/*.test.js' '!node_modules' -c 'clear; jest' ",
    "compile": "babel src --out-dir dist",
    "build": "npm run compile && pkg dist/index.js -t node8-macos-x64 -o bin/server",
    "build-for-all": "pkg index.js -t node8-macos-x64,node8-linux-x64,node8-win-x64 -o bin/server"
  },
  "dependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chokidar-cli": "^1.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "pkg": "^4.3.4",
    "test": "^0.6.0"
  }
}
